{
  "name": "@poda/core",
  "version": "1.0.0",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/index.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "concurrently --raw \"rolldown --config ./rolldown.config.js --watch\" \"pnpm dev:dts\"",
    "dev:dts": "tsc -w --preserveWatchOutput && build:dts",
    "build": "rolldown -c",
    "build:dts": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "lint": "eslint .",
    "test": "NODE_NO_WARNINGS=1 jest --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/modifiers": "9.0.0",
    "@dnd-kit/sortable": "10.0.0",
    "classnames": "^2.5.1",
    "date-fns": "4.1.0",
    "immer": "10.1.1",
    "mousetrap": "1.6.5",
    "zustand": "5.0.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@poda/eslint-config": "workspace:*",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/mousetrap": "1.6.15",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-watch-typeahead": "2.2.2"
  }
}